<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<FactionDef ParentName="FactionBase">
    <defName>guy762_KotORFaction_HuttCartel</defName>
    <label>Hutt Clan</label>
    <description>A criminal and business syndicate ran by a clan of Hutts but operated by many different species, slaves or otherwise.</description>
    <pawnSingular>scum</pawnSingular>
    <pawnsPlural>scum</pawnsPlural>
    <categoryTag>Pirate</categoryTag>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <maxCountAtGameStart>2</maxCountAtGameStart>
	<listOrderPriority>765</listOrderPriority>
	<configurationListOrderPriority>765</configurationListOrderPriority>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
	<hostileToFactionlessHumanlikes>false</hostileToFactionlessHumanlikes>
    <factionIconPath>World/HuttClans</factionIconPath>
    <colorSpectrum>
      <li>RGB(150,115,65)</li>
      <li>RGB(120,120,50)</li>
    </colorSpectrum>
    <permanentEnemy>false</permanentEnemy>
    <factionNameMaker>guy762_NamerFaction_HuttCartel</factionNameMaker>
    <settlementNameMaker>NamerSettlementPirate</settlementNameMaker>
	<techLevel>Spacer</techLevel>
	<allowedArrivalTemperatureRange>-40~45</allowedArrivalTemperatureRange>
    <allowedCultures>
		<li>Kriminul</li>
	</allowedCultures>
	
