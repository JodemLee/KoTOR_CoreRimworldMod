<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  
	
	<ThingDef ParentName="KotORArmorModdable_soft">
    <defName>guy762_CombatSuit_trando</defName>
    <label>Trandoshan Slaver Mesh Suit</label>
    <description>When king Trossk of Trandosha ordered a series of brutal galaxy-wide slave raids over a century ago, these flexible suits became a common - and feared - sight among the unprotected Outer Rim colonies.</description>
    <graphicData>
      <texPath>Apparel/CombatSuit/echani/Apparel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <colorTwo>(130,60,60)</colorTwo>
    </graphicData>
    <statBases>
		<Mass>5</Mass>
		<MarketValue>500</MarketValue>
		<MaxHitPoints>225</MaxHitPoints>
		<ArmorRating_Sharp>0.4</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
		<ArmorRating_Heat>0.5</ArmorRating_Heat>
		<EquipDelay>3</EquipDelay>
	</statBases>
	<equippedStatOffsets>
		<IncomingDamageFactor>-0.05</IncomingDamageFactor>
		<HuntingStealth>0.3</HuntingStealth>              <!---->
	</equippedStatOffsets>
    <apparel>
		<layers>
			<li>Middle</li>
		</layers>
		<bodyPartGroups>
			<li>Torso</li>
			<li>Neck</li>
			<li>Shoulders</li>
		</bodyPartGroups>
		<tags Inherit="False">
			<li>KotORArmor_light</li>
			<li>SaV_apparel_trandoslaver</li>
		</tags>
		<wornGraphicPath>Apparel/CombatSuit/echani/Apparel</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
	<tradeTags>
		<li>guy762TT_KotORArmor</li>
		<li>guy762TT_KotORAntique</li>
	</tradeTags>
	<colorGenerator Class="ColorGenerator_Options" Inherit="False">
		<options>
			<li>
				<weight>10</weight>
				<only>(205,205,100)</only>
			</li>
		</options>
	</colorGenerator>
	<comps>
	  <li Class="ModularWeapons.CompProperties_ModularParts">
		<compClass>ModularWeapons.CompModularParts</compClass>
		<partKinds>
		  <li>guy762_slot_underlay</li>
		</partKinds>
		<randomizeParts>true</randomizeParts>
	  </li>
	</comps>
  </ThingDef>
	
	
	<ThingDef ParentName="KotORArmorModdable_hard">
		<defName>guy762_MidBattleArmor_trando</defName>
		<label>Trandoshan battle armor</label>
		<techLevel>Spacer</techLevel>
		<description>While most Trandoshan hunters prefer to travel light, heavier armor such as this is used by mercenary commando units who engage in more direct combat.</description>
		<graphicData>
			<texPath>Apparel/MidBattleArmor/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<colorTwo>(175,90,50)</colorTwo>
		</graphicData>
		<statBases>
			<Mass>12</Mass>
			<MarketValue>6400</MarketValue>
			<MaxHitPoints>475</MaxHitPoints>
      		<ArmorRating_Sharp>0.85</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.7</ArmorRating_Blunt>
			<ArmorRating_Heat>0.95</ArmorRating_Heat>
			<EquipDelay>10</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.4</MoveSpeed>
			<MeleeDodgeChance>-0.4</MeleeDodgeChance>
			<IncomingDamageFactor>-0.15</IncomingDamageFactor>
			<HuntingStealth>0.1</HuntingStealth>              <!---->
		</equippedStatOffsets>
		<apparel Inherit="False">
			<tags>
				<li>KotORArmor_heavy</li>
				<li>SaV_apparel_trandomerc</li>
			</tags>
			<wornGraphicPath>Apparel/MidBattleArmor/Apparel</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Middle</li>
				<li>OnSkin</li>
			</layers>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Neck</li>
				<li>Arms</li>
				<li>Hands</li>
				<li>Legs</li>
				<li>Feet</li>
			</bodyPartGroups>
			<useDeflectMetalEffect>false</useDeflectMetalEffect>
		</apparel>
		<tradeTags>
			<li>guy762TT_KotORArmor</li>
		</tradeTags>
		<colorGenerator Class="ColorGenerator_Options" Inherit="False">
			<options>
				<li>
					<weight>15</weight>
					<only>(175,170,165)</only>
				</li>
			</options>
		</colorGenerator>
		
		<comps>
		  <li Class="ModularWeapons.CompProperties_ModularParts">
			<compClass>ModularWeapons.CompModularParts</compClass>
			<partKinds>
			  <li>guy762_slot_underlay</li>
			  <li>guy762_slot_overlay_hvy</li>
			</partKinds>
			<randomizeParts>true</randomizeParts>
		  </li>
		</comps>
	</ThingDef>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
